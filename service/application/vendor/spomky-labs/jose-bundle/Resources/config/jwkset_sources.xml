<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="jose.jwk_set_source.jwkset" class="SpomkyLabs\JoseBundle\DependencyInjection\Source\JWKSetSource\JWKSet" public="false">
            <tag name="jose.jwk_set_source" />
        </service>
        <service id="jose.jwk_set_source.keys" class="SpomkyLabs\JoseBundle\DependencyInjection\Source\JWKSetSource\Keys" public="false">
            <tag name="jose.jwk_set_source" />
        </service>
        <service id="jose.jwk_set_source.jku" class="SpomkyLabs\JoseBundle\DependencyInjection\Source\JWKSetSource\JKU" public="false">
            <tag name="jose.jwk_set_source" />
        </service>
        <service id="jose.jwk_set_source.x5u" class="SpomkyLabs\JoseBundle\DependencyInjection\Source\JWKSetSource\X5U" public="false">
            <tag name="jose.jwk_set_source" />
        </service>
        <service id="jose.jwk_set_source.auto" class="SpomkyLabs\JoseBundle\DependencyInjection\Source\JWKSetSource\RandomKeySet" public="false">
            <tag name="jose.jwk_set_source" />
        </service>
        <service id="jose.jwk_set_source.jwksets" class="SpomkyLabs\JoseBundle\DependencyInjection\Source\JWKSetSource\JWKSets" public="false">
            <tag name="jose.jwk_set_source" />
        </service>
        <service id="jose.jwk_set_source.public_jwkset" class="SpomkyLabs\JoseBundle\DependencyInjection\Source\JWKSetSource\PublicJWKSet" public="false">
            <tag name="jose.jwk_set_source" />
        </service>
    </services>
</container>
