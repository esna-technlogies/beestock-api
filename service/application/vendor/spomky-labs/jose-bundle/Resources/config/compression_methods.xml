<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="jose.compression.deflate" class="Jose\Compression\Deflate" public="false">
            <tag name="jose.compression" />
        </service>
        <service id="jose.compression.gzip" class="Jose\Compression\GZip" public="false">
            <tag name="jose.compression" />
        </service>
        <service id="jose.compression.zlib" class="Jose\Compression\ZLib" public="false">
            <tag name="jose.compression" />
        </service>
    </services>
</container>
