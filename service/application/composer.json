{
    "name": "almasry/application",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "symfony/symfony": "3.2.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "symfony/swiftmailer-bundle": "^2.3.10",
        "symfony/monolog-bundle": "^3.0",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0",
        "incenteev/composer-parameter-handler": "^2.0",
        "nelmio/cors-bundle": "^1.5",
        "aws/aws-sdk-php-symfony": "^1.2",
        "jms/serializer-bundle": "^1.1",
        "willdurand/hateoas-bundle": "^1.2",
        "jms/serializer": "^1.5",
        "stof/doctrine-extensions-bundle": "^1.2",
        "alcaeus/mongo-php-adapter": "^1.0",
        "doctrine/mongodb": "^1.4",
        "doctrine/mongodb-odm-bundle": "^3.2",
        "gedmo/doctrine-extensions": "^2.4",
        "doctrine/mongodb-odm": "^1.1",
        "knplabs/knp-paginator-bundle": "^2.5",
        "ramsey/uuid-doctrine": "^1.2",
        "willdurand/geocoder": "^3.3",
        "willdurand/hateoas": "^2.10",
        "spomky-labs/jose-bundle": "^2.2",
        "symfony/event-dispatcher": "^3.2",
        "webmozart/json": "^1.2",
        "sanpi/behatch-contexts": "^2.1",
        "giggsey/libphonenumber-for-php": "^8.3",
        "nelmio/api-doc-bundle": "^2.13",
        "mattketmo/email-checker": "^1.5",
        "sensio/generator-bundle": "^3.1",
        "guzzlehttp/guzzle": "^6.2",
        "csa/guzzle-bundle": "^2.0",
        "friendsofsymfony/jsrouting-bundle": "^1.6",
        "namshi/jose": "^7.2",
        "maxmind/geoip": "^1.1",
        "lexik/jwt-authentication-bundle": "^2.2",
        "friendsofsymfony/user-bundle": "~2.0",
        "php-amqplib/rabbitmq-bundle": "^1.12",
        "sensiolabs/ansi-to-html": "^1.1",
        "hellogerard/jobby": "^3.2",
        "symfony/assetic-bundle": "^2.8"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.2",
        "symfony/debug-bundle": "^3.2",
        "behat/symfony2-extension": "^2.1",
        "behat/behat": "^3.3",
        "behatch/contexts": "^2.6",
        "phpunit/phpunit": "^6.0",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-goutte-driver": "^1.2",
        "nelmio/alice": "^2.2"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    }
}
